{
  "name": "mongo-demo-app",
  "version": "1.0.0",
  "description": "Simple Node.js application with dockerized MongoDB for client demos",
  "main": "server.js",
  "nsolid": {
    "tracingEnabled": true
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:up-build": "docker-compose up -d --build",
    "docker:logs": "docker-compose logs -f",
    "docker:mongo-only": "docker-compose up -d mongodb",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "keywords": [
    "nodejs",
    "mongodb",
    "docker",
    "demo"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/instrumentation-mongodb": "^0.56.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "standard": "^17.1.2"
  }
}
